{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": 0,
    "space-before-function-paren": 0,
    "no-unused-vars": 1,
    "react/prop-types": 1,
    "multiline-ternary": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
